{
  "name": "starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"pnpm run dev:agent\" \"pnpm run dev:ui\"",
    "dev:agent": "mastra dev",
    "dev:ui": "next dev --turbopack",
    "dev:debug": "LOG_LEVEL=debug npm run dev",
    "build:ui": "next build",
    "start:ui": "next start",
    "build:agent": "mastra build",
    "start:agent": "mastra start",
    "lint": "next lint",
    "build": "mastra build && next build",
    "start": "concurrently \"npm:start:agent\" \"npm:start:ui\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:verbose": "jest --verbose",
    "test:silent": "jest --silent",
    "test:integration": "cross-env ENABLE_INTEGRATION_TESTS=true jest --testPathPattern=integration",
    "test:e2e": "cross-env ENABLE_E2E_TESTS=true playwright test",
    "test:e2e:headed": "cross-env ENABLE_E2E_TESTS=true playwright test --headed",
    "test:e2e:debug": "cross-env ENABLE_E2E_TESTS=true playwright test --debug",
    "test:e2e:ui": "cross-env ENABLE_E2E_TESTS=true playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "test:performance": "cross-env ENABLE_PERFORMANCE_TESTS=true jest --testPathPattern=bench",
    "test:load": "node performance/load-tests/api-load.js",
    "test:all": "pnpm test && pnpm test:integration && pnpm test:e2e && pnpm test:performance"
  },
  "dependencies": {
    "@ag-ui/mastra": "0.1.0",
    "@ai-sdk/openai": "^2.0.42",
    "@copilotkit/react-core": "1.10.6",
    "@copilotkit/react-textarea": "1.10.6",
    "@copilotkit/react-ui": "1.10.6",
    "@copilotkit/runtime": "1.10.6",
    "@google/generative-ai": "^0.24.1",
    "@libsql/client": "^0.15.15",
    "@mastra/client-js": "0.14.1",
    "@mastra/core": "0.23.1",
    "@mastra/libsql": "0.16.1",
    "@mastra/mcp": "^0.14.1",
    "@mastra/memory": "0.15.9",
    "@supabase/supabase-js": "^2.75.0",
    "@types/crypto-js": "^4.2.2",
    "@types/nunjucks": "^3.2.6",
    "axios": "^1.12.2",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.1",
    "crypto-js": "^4.2.0",
    "dotenv": "^17.2.3",
    "libsql": "^0.5.22",
    "mastra": "0.17.5",
    "next": ">=15.5.4",
    "nunjucks": "^3.2.4",
    "ollama-ai-provider-v2": "^1.5.0",
    "pdf-parse": "^2.3.12",
    "process": "^0.11.10",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "stream-browserify": "^3.0.0",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.50.0",
    "@tailwindcss/postcss": "^4",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autocannon": "^8.0.0",
    "clinic": "^13.0.0",
    "concurrently": "^9.1.2",
    "cross-env": "^10.1.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jest-watch-typeahead": "^2.2.2",
    "tailwindcss": "^4",
    "ts-jest": "^29.2.5",
    "typescript": "^5"
  },
  "packageManager": "pnpm@10.18.3+sha512.bbd16e6d7286fd7e01f6b3c0b3c932cda2965c06a908328f74663f10a9aea51f1129eea615134bf992831b009eabe167ecb7008b597f40ff9bc75946aadfb08d"
}
